setupFieldMutable(field, useGuava) ::= <<
<if(field.static.undefined)>this.<field.name> = <setupFieldRegardCollectionCopy(field, useGuava)>;<endif>
>>
setupFieldsImmutable(fields, useGuava) ::= <<
<fields: { f|
	<setupFieldImmutable(f, useGuava)>
}>
